Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = True
Option Explicit
Public Property Get Name() As String
End Property
Public Property Let Name(ByVal sName As String)
End Property

Public Function IsDbugEnabled() As Boolean
End Function
Public Function IsInfoEnabled() As Boolean
End Function
Public Function IsWarnEnabled() As Boolean
End Function
Public Function IsErrorEnabled() As Boolean
End Function
Public Function IsFatalEnabled() As Boolean
End Function


Public Sub Dbug(ByVal sMessage As String, Optional ByVal ExceptionNumber As Long, Optional ByVal ExceptionDescription As String, Optional ByVal ExceptionSource As String, Optional ErrorLine As Long)
End Sub
Public Sub Info(ByVal sMessage As String, Optional ByVal ExceptionNumber As Long, Optional ByVal ExceptionDescription As String, Optional ByVal ExceptionSource As String, Optional ErrorLine As Long)
End Sub
Public Sub Warn(ByVal sMessage As String, Optional ByVal ExceptionNumber As Long, Optional ByVal ExceptionDescription As String, Optional ByVal ExceptionSource As String, Optional ErrorLine As Long)
End Sub
Public Sub Error(ByVal sMessage As String, Optional ByVal ExceptionNumber As Long, Optional ByVal ExceptionDescription As String, Optional ByVal ExceptionSource As String, Optional ErrorLine As Long)
End Sub
Public Sub Fatal(ByVal sMessage As String, Optional ByVal ExceptionNumber As Long, Optional ByVal ExceptionDescription As String, Optional ByVal ExceptionSource As String, Optional ErrorLine As Long)
End Sub